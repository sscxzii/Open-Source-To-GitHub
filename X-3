â€Žlocal function createExtraButton(text, posY, callback)
â€Ž	local button = Instance.new("TextButton")
â€Ž	button.Size = UDim2.new(0.9, 0, 0, 35)
â€Ž	button.Position = UDim2.new(0.05, 0, 0, posY)
â€Ž	button.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
â€Ž	button.BackgroundTransparency = 0.3
â€Ž	button.Text = text
â€Ž	button.Font = Enum.Font.GothamBold
â€Ž	button.TextColor3 = Color3.fromRGB(255, 255, 255)
â€Ž	button.TextSize = 14
â€Ž	button.Parent = extraPanel
â€Ž
â€Ž	local corner = Instance.new("UICorner")
â€Ž	corner.CornerRadius = UDim.new(0, 6)
â€Ž	corner.Parent = button
â€Ž
â€Ž	local stroke = Instance.new("UIStroke", button)
â€Ž	stroke.Color = Color3.fromRGB(150, 150, 150)
â€Ž	stroke.Thickness = 2
â€Ž
â€Ž	button.MouseButton1Click:Connect(callback)
â€Žend

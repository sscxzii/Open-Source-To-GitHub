â€Ž-- Create Button Utility
â€Žlocal function createButton(text, positionY, color)
â€Ž    local button = Instance.new("TextButton")
â€Ž    button.Size = UDim2.new(0.9, 0, 0, 35)
â€Ž    button.Position = UDim2.new(0.05, 0, 0, positionY)
â€Ž    button.BackgroundColor3 = color or Color3.fromRGB(70, 70, 70)
â€Ž    button.BackgroundTransparency = 0.3
â€Ž    button.Text = text
â€Ž    button.Font = Enum.Font.GothamBold
â€Ž    button.TextColor3 = Color3.fromRGB(255, 255, 255)
â€Ž    button.TextSize = 16
â€Ž    button.Parent = mainFrame
â€Ž    Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
â€Ž    return button
â€Žend
â€Ž
â€Ž-- Pet ESP
â€Žlocal function glitchLabelEffect(label)
â€Ž    coroutine.wrap(function()
â€Ž        local original = label.TextColor3
â€Ž        for i = 1, 2 do
â€Ž            label.TextColor3 = Color3.new(1, 0, 0)
â€Ž            wait(0.07)
â€Ž            label.TextColor3 = original
â€Ž            wait(0.07)
â€Ž        end
â€Ž    end)()
â€Žend
â€Ž
â€Žlocal function applyEggESP(eggModel, petName)
â€Ž    local existingLabel = eggModel:FindFirstChild("PetBillboard", true)
â€Ž    if existingLabel then existingLabel:Destroy() end
â€Ž    local existingHighlight = eggModel:FindFirstChild("ESPHighlight")
â€Ž    if existingHighlight then existingHighlight:Destroy() end
â€Ž    if not espEnabled then return end
â€Ž
â€Ž    local basePart = eggModel:FindFirstChildWhichIsA("BasePart")
â€Ž    if not basePart then return end
â€Ž
â€Ž    local hatchReady = true
â€Ž    local hatchTime = eggModel:FindFirstChild("HatchTime")
â€Ž    local readyFlag = eggModel:FindFirstChild("ReadyToHatch")
â€Ž
â€Ž    if hatchTime and hatchTime:IsA("NumberValue") and hatchTime.Value > 0 then
â€Ž        hatchReady = false
â€Ž    elseif readyFlag and readyFlag:IsA("BoolValue") and not readyFlag.Value then
â€Ž        hatchReady = false
â€Ž    end
â€Ž
â€Ž    local billboard = Instance.new("BillboardGui")
â€Ž    billboard.Name = "PetBillboard"
â€Ž    billboard.Size = UDim2.new(0, 270, 0, 50)
â€Ž    billboard.StudsOffset = Vector3.new(0, 4.5, 0)
â€Ž    billboard.AlwaysOnTop = true
â€Ž    billboard.MaxDistance = 500
â€Ž    billboard.Parent = basePart
â€Ž
â€Ž    local label = Instance.new("TextLabel")
â€Ž    label.Size = UDim2.new(1, 0, 1, 0)
â€Ž    label.BackgroundTransparency = 1
â€Ž    label.Text = eggModel.Name .. " | " .. petName
â€Ž    if not hatchReady then
â€Ž        label.Text = eggModel.Name .. " | " .. petName .. " (Not Ready)"
â€Ž        label.TextColor3 = Color3.fromRGB(160, 160, 160)
â€Ž        label.TextStrokeTransparency = 0.5
â€Ž    else
â€Ž        label.TextColor3 = Color3.new(1, 1, 1)
â€Ž        label.TextStrokeTransparency = 0
â€Ž    end
â€Ž    label.TextScaled = true
â€Ž    label.Font = Enum.Font.FredokaOne
â€Ž    label.Parent = billboard
â€Ž
â€Ž    glitchLabelEffect(label)
â€Ž
â€Ž    local highlight = Instance.new("Highlight")
â€Ž    highlight.Name = "ESPHighlight"
â€Ž    highlight.FillColor = Color3.fromRGB(255, 200, 0)
â€Ž    highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
â€Ž    highlight.FillTransparency = 0.7
â€Ž    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
â€Ž    highlight.Adornee = eggModel
â€Ž    highlight.Parent = eggModel
â€Žend
â€Ž
â€Žlocal function removeEggESP(eggModel)
â€Ž    local label = eggModel:FindFirstChild("PetBillboard", true)
â€Ž    if label then label:Destroy() end
â€Ž    local highlight = eggModel:FindFirstChild("ESPHighlight")
â€Ž    if highlight then highlight:Destroy() end
â€Žend
â€Ž
â€Žlocal function getPlayerGardenEggs(radius)
â€Ž    local eggs = {}
â€Ž    local char = player.Character or player.CharacterAdded:Wait()
â€Ž    local root = char:FindFirstChild("HumanoidRootPart")
â€Ž    if not root then return eggs end
â€Ž
â€Ž    for _, obj in pairs(Workspace:GetDescendants()) do
â€Ž        if obj:IsA("Model") and petTable[obj.Name] then
â€Ž            local dist = (obj:GetModelCFrame().Position - root.Position).Magnitude
â€Ž            if dist <= (radius or 60) then
â€Ž                if not truePetMap[obj] then
â€Ž                    local pets = petTable[obj.Name]
â€Ž                    local chosen = pets[math.random(1, #pets)]
â€Ž                    truePetMap[obj] = chosen
â€Ž                end
â€Ž                table.insert(eggs, obj)
â€Ž            end
â€Ž        end
â€Ž    end
â€Ž    return eggs
â€Žend
â€Ž
â€Žlocal function randomizeNearbyEggs()
â€Ž    local eggs = getPlayerGardenEggs(60)
â€Ž    for _, egg in ipairs(eggs) do
â€Ž        local pets = petTable[egg.Name]
â€Ž        local chosen = pets[math.random(1, #pets)]
â€Ž        truePetMap[egg] = chosen
â€Ž        applyEggESP(egg, chosen)
â€Ž    end
â€Ž    print("Randomized", #eggs, "eggs.")
â€Žend
â€Ž
â€Žlocal function flashEffect(button)
â€Ž    local originalColor = button.BackgroundColor3
â€Ž    for i = 1, 3 do
â€Ž        button.BackgroundColor3 = Color3.new(1, 1, 1)
â€Ž        wait(0.05)
â€Ž        button.BackgroundColor3 = originalColor
â€Ž        wait(0.05)
â€Ž    end
â€Žend
â€Ž
â€Žlocal function countdownAndRandomize(button)
â€Ž    for i = 10, 1, -1 do
â€Ž        button.Text = "ðŸŽ² Randomize in: " .. i
â€Ž        wait(1)
â€Ž    end
â€Ž    flashEffect(button)
â€Ž    randomizeNearbyEggs()
â€Ž    button.Text = "ðŸŽ² Randomize Pets"
â€Žend
â€Ž
â€Ž-- UI Buttons
â€Žlocal randomizeBtn = createButton("ðŸŽ² Randomize Pets", 50)
â€Žlocal espBtn = createButton("ðŸ‘ï¸ ESP: ON", 90)
â€Žlocal autoBtn = createButton("ðŸ” Auto Randomize: OFF", 130)
â€Ž
â€Ž-- Button Logic
â€ŽrandomizeBtn.MouseButton1Click:Connect(function()
â€Ž    countdownAndRandomize(randomizeBtn)
â€Žend)
â€Ž
â€ŽespBtn.MouseButton1Click:Connect(function()
â€Ž    espEnabled = not espEnabled
â€Ž    espBtn.Text = espEnabled and "ðŸ‘ï¸ ESP: ON" or "ðŸ‘ï¸ ESP: OFF"
â€Ž    for _, egg in pairs(getPlayerGardenEggs(60)) do
â€Ž        if espEnabled then
â€Ž            applyEggESP(egg, truePetMap[egg])
â€Ž        else
â€Ž            removeEggESP(egg)
â€Ž        end
â€Ž    end
â€Žend)
â€Ž
â€ŽautoBtn.MouseButton1Click:Connect(function()
â€Ž    autoRunning = not autoRunning
â€Ž    autoBtn.Text = autoRunning and "ðŸ” Auto Randomize: ON" or "ðŸ” Auto Randomize: OFF"
â€Ž    coroutine.wrap(function()
â€Ž        while autoRunning do
â€Ž            countdownAndRandomize(randomizeBtn)
â€Ž            for _, petName in pairs(truePetMap) do
â€Ž                if bestPets[petName] then
â€Ž                    autoRunning = false
â€Ž                    autoBtn.Text = "ðŸ” Auto Randomize: OFF"
â€Ž                    return
â€Ž                end
â€Ž            end
â€Ž            wait(1)
â€Ž        end
â€Ž    end)()
â€Žend)
â€Ž
â€Ž-- Initial ESP
â€Žfor _, egg in pairs(getPlayerGardenEggs(60)) do
â€Ž    applyEggESP(egg, truePetMap[egg])
â€Žend
â€Ž
â€Ž-- Draggable Main Frame
â€Žlocal dragging = false
â€Žlocal dragInput, mousePos, framePos
â€Ž
â€ŽmainFrame.InputBegan:Connect(function(input)
â€Ž    if input.UserInputType == Enum.UserInputType.MouseButton1 then
â€Ž        dragging = true
â€Ž        mousePos = input.Position
â€Ž        framePos = mainFrame.Position
â€Ž
â€Ž        input.Changed:Connect(function()
â€Ž            if input.UserInputState == Enum.UserInputState.End then
â€Ž                dragging = false
â€Ž            end
â€Ž        end)
â€Ž    end
â€Žend)
